#include <iostream>
using namespace std;


int main()
{
	int MyIntArray[10];

	for (int i =0; i<10; i++)
	{
		MyIntArray[i] = i;
		//cout << MyIntArray[i] << endl;
	}

	cout << "The first element in myintarray is : " << MyIntArray[0] << endl;
	cout << "The fifth element in MyIntArray is : " << MyIntArray[4] << endl;
	
	int MyArray[5] = {1,2,3,4,5};
	cout << "MyArry[0] = " << MyArray[0] << endl;
	for (int i=0; i<5; i++)
	{
		cout << MyArray[i] << endl;
	}

	system("pause");

}
